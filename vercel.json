{
  "buildCommand": "turbo build",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "./",
  "builds": [
    {
      "src": "apps/frontend/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "apps/backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "apps/frontend/$1"
    }
  ]
}
